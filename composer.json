{
    "name": "halcyon/saas-platform",
    "type": "project",
    "description": "SaaS Platform",
    "license": "proprietary",
    "require": {
        "php": "^8.1.9",
        "composer-runtime-api": "^2.2.2",
        "artificertech/filament-multi-context": "^2.0",
        "aws/aws-sdk-php": "^3.252",
        "bacon/bacon-qr-code": "^2.0",
        "fakerphp/faker": "^1.21",
        "filament/filament": "^2.16.6",
        "filament/spatie-laravel-media-library-plugin": "^2.17",
        "filament/spatie-laravel-settings-plugin": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.11",
        "laravel/pennant": "^1.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^2.28",
        "league/flysystem-aws-s3-v3": "^3.13",
        "paypal/rest-api-sdk-php": "^1.6",
        "maatwebsite/excel": "^3.1.48",
        "pragmarx/google2fa": "^8.0",
        "saade/filament-laravel-log": "^1.2",
        "shuvroroy/filament-spatie-laravel-health": "^1.5",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-json-api-paginate": "^1.12",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-route-attributes": "^1.11",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-validation-rules": "^3.3",
        "stancl/tenancy": "^3.6",
        "stechstudio/filament-impersonate": "^2.10",
        "timacdonald/json-api": "^1.0.0-beta.4",
        "victord11/ssl-certification-health-check": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.18",
        "nunomaduro/collision": "^7.2",
        "nunomaduro/larastan": "^2.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "pestphp/pest-plugin-mock": "^2.0",
        "phpstan/phpstan": "^1.8",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.31",
        "worksome/request-factories": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Support\\": "support/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan optimize:clear --ansi",
            "@php artisan settings:clear-discovered --ansi",
            "@php artisan settings:clear-cache --ansi"
        ],
        "cache-all": [
            "composer dumpautoload -o",
            "@php artisan event:cache --ansi",
            "@php artisan config:cache --ansi",
            "@php artisan route:cache --ansi",
            "@php artisan view:cache --ansi",
            "@php artisan settings:discover --ansi"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "ide-helper": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "ide-helper-model": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi",
            "@php artisan ide-helper:models -r -W --ansi",
            "@php artisan tenants:run ide-helper:models --option=\"smart-reset=1\" --option=\"write=1\" --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "format-dirty": "vendor/bin/pint --dirty",
        "format-dry-run": "vendor/bin/pint --test",
        "test": "vendor/bin/pest --parallel",
        "test-no-timeout": ["Composer\\Config::disableProcessTimeout", "vendor/bin/pest --parallel"],
        "test-coverage": "vendor/bin/pest --parallel --coverage-html build/coverage",
        "x-ray": "vendor/bin/x-ray app database domain resources routes tests"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.9"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
