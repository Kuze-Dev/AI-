{
    "name": "halcyon/saas-platform",
    "type": "project",
    "description": "SaaS Platform",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "akaunting/laravel-money": "^5.2.2",
        "awcodes/filament-tiptap-editor": "^3.4.22",
        "aws/aws-sdk-php": "^3.334.7",
        "bacon/bacon-qr-code": "^3.0.1",
        "barryvdh/laravel-dompdf": "^3.0.0",
        "fakerphp/faker": "^1.24.1",
        "filament/filament": "^3.2.130",
        "filament/spatie-laravel-media-library-plugin": "^3.2.130",
        "filament/spatie-laravel-settings-plugin": "^3.2.130",
        "flowframe/laravel-trend": "^0.3",
        "guzzlehttp/guzzle": "^7.9.2",
        "indeev/laravel-schedule-calendar": "^1.3",
        "laravel/framework": "^11.35.1",
        "laravel/horizon": "^5.30.0",
        "laravel/pennant": "^1.13",
        "laravel/sanctum": "^4.0.6",
        "laravel/tinker": "^2.10",
        "laravel/vapor-cli": "^1.65.2",
        "laravel/vapor-core": "^2.37.8",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "lloricode/php-timezone": "^1.1.1",
        "lloricode/spatie-image-optimizer-health-check": "^2.0.3",
        "maatwebsite/excel": "^3.1.61",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.3.3",
        "pragmarx/google2fa": "^8.0.3",
        "sentry/sentry-laravel": "^4.10.1",
        "shuvroroy/filament-spatie-laravel-health": "^2.2.0",
        "spatie/array-to-xml": "^3.4.0",
        "spatie/eloquent-sortable": "^4.4.0",
        "spatie/laravel-activitylog": "^4.9.1",
        "spatie/laravel-json-api-paginate": "^1.16.0",
        "spatie/laravel-medialibrary": "^11.11",
        "spatie/laravel-permission": "^6.10.1",
        "spatie/laravel-query-builder": "^6.2.2",
        "spatie/laravel-queueable-action": "^2.15.0",
        "spatie/laravel-route-attributes": "^1.25.0",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-validation-rules": "^3.4.0",
        "spatie/once": "^3.1",
        "srmklive/paypal": "^3.0.32",
        "stancl/tenancy": "^3.8.5",
        "stechstudio/filament-impersonate": "^3.15.0",
        "stripe/stripe-php": "^16.3",
        "timacdonald/json-api": "^1.0.0-beta.6",
        "victord11/ssl-certification-health-check": "^1.0.5",
        "vyuldashev/xml-to-array": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14.9",
        "barryvdh/laravel-ide-helper": "^3.2.2",
        "beyondcode/helo-laravel": "^1.8",
        "canvural/larastan-strict-rules": "^3.0.0",
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.0.2",
        "laravel/pint": "^1.18.3",
        "laravel/sail": "^1.39.1",
        "nunomaduro/collision": "^8.5.0",
        "pestphp/pest": "^3.7.1",
        "pestphp/pest-plugin-faker": "^3.0.0",
        "pestphp/pest-plugin-laravel": "^3.0.0",
        "pestphp/pest-plugin-livewire": "^3.0.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "rector/rector": "^2.0.3",
        "spatie/laravel-ignition": "^2.9.0",
        "spatie/laravel-ray": "^1.39.0",
        "worksome/request-factories": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Support\\": "support/",
            "HalcyonAgile\\FilamentImport\\": "packages/halcyon-agile/filament-import/src/",
            "HalcyonAgile\\FilamentExport\\": "packages/halcyon-agile/filament-export/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --provider=\"romanzipp\\QueueMonitor\\Providers\\QueueMonitorProvider\" --tag=assets --ansi --force",
            "@php artisan vendor:publish --tag=horizon-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --provider=\"romanzipp\\QueueMonitor\\Providers\\QueueMonitorProvider\" --tag=assets --ansi --force",
            "@php artisan vendor:publish --tag=horizon-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan optimize:clear --ansi",
            "@php artisan settings:clear-discovered --ansi",
            "@php artisan settings:clear-cache --ansi",
            "@php artisan icons:clear --ansi",
            "@php artisan filament:clear-cached-components --ansi"
        ],
        "cache-all": [
            "composer dumpautoload -o",
            "@php artisan event:cache --ansi",
            "@php artisan config:cache --ansi",
            "@php artisan route:cache --ansi",
            "@php artisan view:cache --ansi",
            "@php artisan settings:discover --ansi",
            "@php artisan icons:cache --ansi",
            "@php artisan filament:cache-components --ansi"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "ide-helper": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "ide-helper-model": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi",
            "@php artisan ide-helper:models -r -W --ansi",
            "@php artisan tenants:run ide-helper:models --option=\"smart-reset=1\" --option=\"write=1\" --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-no-memory-limit": "vendor/bin/phpstan analyse --memory-limit=-1",
        "analyse-generate-baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "format": "vendor/bin/pint",
        "format-dirty": "vendor/bin/pint --dirty",
        "format-dry-run": "vendor/bin/pint --test",
        "test": "vendor/bin/pest --parallel",
        "test-no-timeout": ["Composer\\Config::disableProcessTimeout", "vendor/bin/pest --parallel"],
        "test-coverage": "vendor/bin/pest --parallel --coverage-html build/coverage",
        "refactor": "vendor/bin/rector process",
        "refactor-dry-run": "vendor/bin/rector process --dry-run",
        "remove-ray": [
            "vendor/bin/remove-ray.sh app",
            "vendor/bin/remove-ray.sh database",
            "vendor/bin/remove-ray.sh domain",
            "vendor/bin/remove-ray.sh resources",
            "vendor/bin/remove-ray.sh routes",
            "vendor/bin/remove-ray.sh support",
            "vendor/bin/remove-ray.sh tests"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
