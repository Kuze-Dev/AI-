{
    "name": "halcyon/saas-platform",
    "type": "project",
    "description": "SaaS Platform",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "akaunting/laravel-money": "^6.0.2",
        "awcodes/filament-tiptap-editor": "^3.5.9",
        "awcodes/filament-versions": "^2.0.1",
        "bacon/bacon-qr-code": "^3.0.1",
        "barryvdh/laravel-dompdf": "^3.1.1",
        "cheesegrits/filament-google-maps": "^3.0.18",
        "fakerphp/faker": "^1.24.1",
        "filament/filament": "^3.3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.3.0",
        "filament/spatie-laravel-settings-plugin": "^3.3.0",
        "flowframe/laravel-trend": "^0.4",
        "guzzlehttp/guzzle": "^7.9.2",
        "indeev/laravel-schedule-calendar": "^1.3",
        "juliomotol/filament-password-confirmation": "^1.2",
        "laravel/framework": "^11.44.0",
        "laravel/horizon": "^5.30.3",
        "laravel/pennant": "^1.16",
        "laravel/sanctum": "^4.0.8",
        "laravel/tinker": "^2.10.1",
        "laravel/vapor-cli": "^1.65.6",
        "laravel/vapor-core": "^2.37.11",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "lloricode/php-timezone": "^1.1.1",
        "lloricode/spatie-image-optimizer-health-check": "^2.0.4",
        "maatwebsite/excel": "^3.1.64",
        "malzariey/filament-daterangepicker-filter": "^3.2.1",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.4.0",
        "pragmarx/google2fa": "^8.0.3",
        "pxlrbt/filament-environment-indicator": "^2.1",
        "sentry/sentry-laravel": "^4.13.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.3.5",
        "spatie/array-to-xml": "^3.4.0",
        "spatie/eloquent-sortable": "^4.4.2",
        "spatie/laravel-activitylog": "^4.10.1",
        "spatie/laravel-json-api-paginate": "^1.16.3",
        "spatie/laravel-medialibrary": "^11.12.7",
        "spatie/laravel-permission": "^6.16.0",
        "spatie/laravel-query-builder": "^6.3.1",
        "spatie/laravel-queueable-action": "^2.16.2",
        "spatie/laravel-route-attributes": "^1.25.1",
        "spatie/laravel-sluggable": "^3.7.4",
        "spatie/laravel-validation-rules": "^3.4.3",
        "srmklive/paypal": "^3.0.40",
        "stancl/tenancy": "^3.9.0",
        "stechstudio/filament-impersonate": "^3.15.0",
        "stripe/stripe-php": "^16.6",
        "timacdonald/json-api": "^1.0.0-beta.6",
        "victord11/ssl-certification-health-check": "^1.0.5",
        "vyuldashev/xml-to-array": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.2",
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "beyondcode/helo-laravel": "^1.8",
        "canvural/larastan-strict-rules": "^3.0",
        "driftingly/rector-laravel": "^2.0.2",
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.21.0",
        "laravel/sail": "^1.41.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.6.1",
        "pestphp/pest": "^3.7.4",
        "pestphp/pest-plugin-faker": "^3.0.0",
        "pestphp/pest-plugin-laravel": "^3.1.0",
        "pestphp/pest-plugin-livewire": "^3.0.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.6",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "rector/rector": "^2.0.9",
        "spatie/laravel-ignition": "^2.9.1",
        "spatie/laravel-ray": "^1.39.1",
        "ticketswap/phpstan-error-formatter": "^1.1.4",
        "worksome/request-factories": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Support\\": "support/",
            "HalcyonAgile\\FilamentImport\\": "packages/halcyon-agile/filament-import/src/",
            "HalcyonAgile\\FilamentExport\\": "packages/halcyon-agile/filament-export/src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --provider=\"romanzipp\\QueueMonitor\\Providers\\QueueMonitorProvider\" --tag=assets --ansi --force",
            "@php artisan vendor:publish --tag=horizon-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --provider=\"romanzipp\\QueueMonitor\\Providers\\QueueMonitorProvider\" --tag=assets --ansi --force",
            "@php artisan vendor:publish --tag=horizon-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi"
        ],
        "ide-helper-model": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi",
            "@php artisan ide-helper:meta --ansi",
            "@php artisan ide-helper:models -r -W --ansi",
            "@php artisan tenants:run ide-helper:models --option=\"smart-reset=1\" --option=\"write=1\" --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-pro": "vendor/bin/phpstan analyse --pro",
        "analyse-pro-no-timeout": ["Composer\\Config::disableProcessTimeout", "vendor/bin/phpstan analyse --pro"],
        "analyse-no-memory-limit": "vendor/bin/phpstan analyse --memory-limit=-1",
        "analyse-generate-baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "format": "vendor/bin/pint",
        "format-dirty": "vendor/bin/pint --dirty",
        "format-dry-run": "vendor/bin/pint --test",
        "test": "vendor/bin/pest -p --processes=6",
        "test-ci": ["Composer\\Config::disableProcessTimeout", "vendor/bin/pest --parallel"],
        "test-no-timeout": ["Composer\\Config::disableProcessTimeout", "vendor/bin/pest --processes=6 --parallel"],
        "test-stop-on-f": "vendor/bin/pest -p --processes=6 --stop-on-failure --stop-on-error",
        "test-type-coverage": "vendor/bin/pest --processes=6 --type-coverage",
        "test-coverage": [
            "Composer\\Config::disableProcessTimeout",
            "XDEBUG_MODE=coverage vendor/bin/pest -p --processes=6 --coverage --coverage-html build/coverage-html --coverage"
        ],
        "refactor": "vendor/bin/rector process",
        "refactor-dry-run": "vendor/bin/rector process --dry-run",
        "remove-ray": [
            "vendor/bin/remove-ray.sh app",
            "vendor/bin/remove-ray.sh database",
            "vendor/bin/remove-ray.sh domain",
            "vendor/bin/remove-ray.sh resources",
            "vendor/bin/remove-ray.sh routes",
            "vendor/bin/remove-ray.sh support",
            "vendor/bin/remove-ray.sh tests"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.3",
            "ext-pcntl": "8.3",
            "ext-posix": "8.3"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
